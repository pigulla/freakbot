{
  "name": "freakbot",
  "version": "0.0.2",
  "description": "The Freakbrothers Discord Bot",
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "npm run eslint && npm run prettier",
    "format": "prettier --write src test && eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prettier": "prettier --check src test",
    "eslint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "test": "npm run lint && npm run mocha && npm audit",
    "mocha": "nyc mocha 'test/**/*.spec.ts'",
    "codecov": "bash -c 'bash <(curl https://codecov.io/bash) -Z -f ./coverage/lcov.info'"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "config": "^3.3.1",
    "discord.js": "^12.2.0",
    "discord.js-commando": "github:discordjs/Commando#bdbd84e2a978322f3af12c21e729eb7b7cd4a3dc",
    "lodash.sample": "^4.2.1",
    "node-pre-gyp": "^0.14.0",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "read-pkg-up": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "simple-git": "^2.3.0",
    "string-argv": "^0.3.1",
    "ts-error": "^1.0.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/lodash.sample": "^4.2.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "@types/pino": "^6.0.0",
    "@types/semver": "^7.1.0",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@types/supertest": "^2.0.9",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.13.1",
    "typescript": "^3.7.4"
  }
